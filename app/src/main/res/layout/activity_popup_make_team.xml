<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/make_team_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    android:orientation="vertical">

    <!-- 타이틀바 -->

    <TextView
        android:id="@+id/make_team_textView_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:background="#00FFFFFF"
        android:gravity="center"
        android:text="팀 구성"
        android:textColor="#000000"
        android:textSize="35dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/make_team_editText_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="15"
        android:selectAllOnFocus="true"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/make_team_backgroud"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/make_team_team_logo"
        app:layout_constraintTop_toTopOf="@+id/make_team_backgroud" />

    <TextView
        android:id="@+id/make_team_backgroud"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/custom_team_cardview_backgroud"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline5" />

    <TextView
        android:id="@+id/make_team_imageX"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="이미지 삭제"
        app:layout_constraintEnd_toEndOf="@+id/make_team_team_logo"
        app:layout_constraintStart_toStartOf="@+id/make_team_backgroud"
        app:layout_constraintTop_toBottomOf="@+id/make_team_backgroud" />

    <ImageView
        android:id="@+id/make_team_team_logo"
        android:layout_width="78dp"
        android:layout_height="78dp"
        android:src="@drawable/nothing"
        app:layout_constraintBottom_toBottomOf="@+id/make_team_backgroud"
        app:layout_constraintEnd_toEndOf="@+id/make_team_backgroud"
        app:layout_constraintStart_toStartOf="@+id/make_team_backgroud"
        app:layout_constraintTop_toTopOf="@+id/make_team_backgroud" />

    <ImageView
        android:id="@+id/make_team_ok"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/checkicon"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.1" />

    <LinearLayout
        android:id="@+id/linearLayout17"
        android:layout_width="0dp"
        android:layout_height="150dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/make_team_imageX">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/make_team_recycler"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="#00FFFFFF" />
    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout18"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout17">

        <LinearLayout
            android:id="@+id/make_team_layout0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:background="#FFFFFF"
            android:orientation="vertical"
            app:layout_constraintEnd_toStartOf="@id/make_team_layout1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="0dp"
                android:background="#FFFFFF">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/make_team_linearlayout0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/make_team_player_image0"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:src="@drawable/ahri"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/make_team_player_name0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:lines="2"
                        android:text="가나다라마"
                        android:textColor="#000000"
                        android:textSize="10dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/make_team_position0"
                        app:layout_constraintTop_toTopOf="@+id/make_team_position0" />

                    <ImageView
                        android:id="@+id/make_team_position0"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/position_top"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/make_team_player_image0" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/make_team_layout1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:orientation="vertical"
            app:layout_constraintEnd_toStartOf="@id/make_team_layout2"
            app:layout_constraintStart_toEndOf="@id/make_team_layout0"
            app:layout_constraintTop_toTopOf="@+id/make_team_layout0">

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="0dp"
                android:background="#FFFFFF">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/make_team_linearlayout1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/make_team_player_image1"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:src="@drawable/ahri"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/make_team_player_name1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:lines="1"
                        android:text="페이커"
                        android:textColor="#000000"
                        android:textSize="10dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/make_team_position1"
                        app:layout_constraintTop_toTopOf="@+id/make_team_position1" />

                    <ImageView
                        android:id="@+id/make_team_position1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/position_jg"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/make_team_player_image1" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/make_team_layout2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/make_team_layout1"
            app:layout_constraintTop_toTopOf="@+id/make_team_layout0">

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="0dp"
                android:background="#FFFFFF">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/make_team_linearlayout2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/make_team_player_image2"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:src="@drawable/ahri"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/make_team_player_name2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:lines="1"
                        android:text="페이커"
                        android:textColor="#000000"
                        android:textSize="10dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/make_team_position2"
                        app:layout_constraintTop_toTopOf="@+id/make_team_position2" />

                    <ImageView
                        android:id="@+id/make_team_position2"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/position_mid"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/make_team_player_image2" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

        </LinearLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/make_team_linearlayout3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/make_team_linearlayout4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/make_team_layout0"
            app:layout_constraintVertical_bias="0.19999999">

            <TextView
                android:id="@+id/make_team_player_name3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:lines="1"
                android:text="페이커"
                android:textColor="#000000"
                android:textSize="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/make_team_position3"
                app:layout_constraintTop_toTopOf="@+id/make_team_position3" />

            <ImageView
                android:id="@+id/make_team_player_image3"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:src="@drawable/ahri"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/make_team_position3"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/position_bot"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/make_team_player_image3" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/make_team_linearlayout4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="@+id/make_team_linearlayout3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/make_team_linearlayout3"
            app:layout_constraintTop_toTopOf="@+id/make_team_linearlayout3">

            <ImageView
                android:id="@+id/make_team_player_image4"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:src="@drawable/ahri"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/make_team_player_name4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:lines="1"
                android:text="페이커"
                android:textColor="#000000"
                android:textSize="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/make_team_position4"
                app:layout_constraintTop_toTopOf="@+id/make_team_position4" />

            <ImageView
                android:id="@+id/make_team_position4"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/position_sup"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/make_team_player_image4" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- //end 타이틀바 -->

    <!-- Notice -->

    <!-- Notice -->

    <!-- 닫기 버튼 -->
    <!--// 닫기 버튼 -->
</androidx.constraintlayout.widget.ConstraintLayout>
